<?xml version="1.0" encoding="utf-8" ?>

<!-- Contient les clefs du web.config qui doivent être modifiees en fonction des environnements Dev / Int / PreProd / Prod -->

<parameters>


  <!--Parametrage Compte Active Directory-->
  <parameter
      name="SecurityActiveDirectoryDomaine"
      description="Adresse LDAP"
      defaultvalue="__SecurityActiveDirectoryDomaine__" tags="">
    <parameterentry
        kind="XmlFile"
        scope="\\web.config$"
        match="/configuration/appSettings/add[@key='Security:ActiveDirectory:Domaine']/@value">
    </parameterentry>
  </parameter>


  <parameter
      name="SecurityActiveDirectorySystemLogin"
      description="Compte de service LDAP"
      defaultvalue="__SecurityActiveDirectorySystemLogin__" tags="">
    <parameterentry
        kind="XmlFile"
        scope="\\web.config$"
        match="/configuration/appSettings/add[@key='Security:ActiveDirectory:System:Login']/@value">
    </parameterentry>
  </parameter>


  <parameter
      name="SecurityActiveDirectorySystemPassword"
      description="Mot de passe du compte de service LDAP"
      defaultvalue="__SecurityActiveDirectorySystemPassword__" tags="">
    <parameterentry
        kind="XmlFile"
        scope="\\web.config$"
        match="/configuration/appSettings/add[@key='Security:ActiveDirectory:System:Password']/@value">
    </parameterentry>
  </parameter>





  <!-- Parametrage du serveur de mail-->
  <parameter
      name="EmailServerAddress"
      description="Adresse du server SMTP"
      defaultvalue="__EmailServerAddress__" tags="">
    <parameterentry
        kind="XmlFile"
        scope="\\web.config$"
        match="/configuration/appSettings/add[@key='eMail:Server:Adress']/@value">
    </parameterentry>
  </parameter>


  <parameter
      name="EmailServerPort"
      description="Port du server SMTP"
      defaultvalue="__EmailServerPort__" tags="">
    <parameterentry
        kind="XmlFile"
        scope="\\web.config$"
        match="/configuration/appSettings/add[@key='eMail:Server:Port']/@value">
    </parameterentry>
  </parameter>



  <!-- Fred Import/Export API Endpoints -->
  <parameter
    name="FredIEWebApiUrl"
    description="Adresse du service FredIE Api"
    defaultvalue="__FredIEWebApiUrl__" tags="">
    <parameterentry
      kind="XmlFile"
      scope="\\web.config$"
      match="/configuration/externalServices/importExport/@url">
    </parameterentry>
  </parameter>

  <!-- Fred Import/Export Service Accounts -->

  <parameter
    name="FredIEServiceAccountLogin"
    description="Nom du compte de service FredIE Api"
    defaultvalue="__FredIEServiceAccountLogin__" tags="">
    <parameterentry
      kind="XmlFile"
      scope="\\web.config$"
      match="/configuration/externalServices/importExport/serviceAccounts/add[@groupCode='Default']/@username">
    </parameterentry>
  </parameter>

  <parameter
    name="FredIEServiceAccountPassword"
    description="Mot de passe du compte de service FredIE Api"
    defaultvalue="__FredIEServiceAccountPassword__" tags="">
    <parameterentry
      kind="XmlFile"
      scope="\\web.config$"
      match="/configuration/externalServices/importExport/serviceAccounts/add[@groupCode='Default']/@password">
    </parameterentry>
  </parameter>

