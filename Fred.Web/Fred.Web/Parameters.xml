  <!-- Parametrage d'AppInsight -->
  <parameter
      name="FredAppInsightKey"
      description="Clé API pour l'environnement"
      defaultvalue="__FredAppInsightKey__" tags="">
    <parameterentry
        kind="XmlFile"
        scope="\\web.config$"
          match="/configuration/appSettings/add[@key='Microsoft:ApplicationInsight:Key']/@value">
    </parameterentry>
  </parameter>


  <!-- Parametrage des logs -->
  <parameter
      name="LogFilePath"
      description="Chemin du fichier de log"
      defaultvalue="__LogFilePath__" tags="">
    <parameterentry
        kind="XmlFile"
        scope="\\web.config$"
        match="/configuration/nlog/targets/target[@name='logfile']/@fileName">
    </parameterentry>
  </parameter>


  <!-- Parametrage du DbLink Buyer-->
  <parameter
      name="BuyerServerAddress"
      description="Adresse du server sql de Buyer"
      defaultvalue="__BuyerServerAddress__" tags="">
    <parameterentry
        kind="XmlFile"
        scope="\\web.config$"
        match="/configuration/appSettings/add[@key='Buyer:Server:Address']/@value">
    </parameterentry>
  </parameter>

  <parameter
    name="BuyerServerInstance"
    description="Instance sur le server sql de Buyer"
    defaultvalue="__BuyerServerInstance__" tags="">
    <parameterentry
        kind="XmlFile"
        scope="\\web.config$"
        match="/configuration/appSettings/add[@key='Buyer:Server:Instance']/@value">
    </parameterentry>
  </parameter>

  <parameter
  name="BuyerServerDatabase"
  description="Nom de la base Buyer"
  defaultvalue="__BuyerServerDatabase__" tags="">
    <parameterentry
        kind="XmlFile"
        scope="\\web.config$"
        match="/configuration/appSettings/add[@key='Buyer:Server:Database']/@value">
    </parameterentry>
  </parameter>

  <!-- Parametrage du dossier de pièces jointes -->
  <parameter
  name="AttachmentFolder"
  description="Dossier où sont enregistrés les Pièces Jointes"
  defaultvalue="__AttachmentFolder__" tags="">
    <parameterentry
        kind="XmlFile"
        scope="\\web.config$"
        match="/configuration/appSettings/add[@key='attachment:folder']/@value">
    </parameterentry>
  </parameter>

</parameters>
