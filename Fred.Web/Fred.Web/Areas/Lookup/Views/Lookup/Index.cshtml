            on-select="$ctrl.onSelectLookup3('lookup3', item)">
      <span class="inputPLSearch inputPLSearchParent lookup-image-plus"></span>
    </lookup>
    <br>
    <div class="title">Le code :</div>
    <xmp>

      <lookup ng-model="$ctrl.CILookup3"
              text="$ctrl.CILookup3.Code"
              search-title="{{$ctrl.searchTitle}}"
              search-placeholder="{{$ctrl.Placeholder}}"
              on-click="$ctrl.initLookup3()"
              on-select="$ctrl.onSelectLookup3('lookup3', item)">
        <span class="inputPLSearch inputPLSearchParent lookup-image-plus"></span>
      </lookup>
    </xmp>

    <div class="title">Information </div>
    <div>Jouer avec les classes sur le span pour modifier l'apparence du boutton"</div>
  </div>




