  @*////////////////////////////////////////////////////////////////////////////////  STANDALONE   ////////////////////////////////////////////////////////////////////////////////
    @*///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*@




  <div class="row bg-success detailLookUp">
