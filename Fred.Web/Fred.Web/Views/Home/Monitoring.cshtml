<style>
  .fontdisplayer ul {
    padding: 0;
    margin: 0;
    list-style-type: none;
  }

   .fontdisplayer li {
    margin-left: 2px;
    float: left; /*pour IE*/
    height:125px;
    width:125px;
    border:1px solid #e1e1e1;
    text-align:center;
    cursor:pointer;
  }

   .fontdisplayer li span{
    font-size:25px;
  }

   .fontdisplayer li code{
    display:block;
    background-color:transparent;
    cursor:text;
  }

   .fontdisplayer ul li:hover {
    background-color: #f5f5f5;
    border-color:  crimson;
  }
</style>


